{
    "manifest_version": 2,
    "name": "CBD",
    "description": "Cambridge Dictionary Translation",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting",
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    }
}